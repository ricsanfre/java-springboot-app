{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "demo-app",
      "image": "ricsanfre/spring-boot-demo-api:01.11.2023.10.34.50",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}